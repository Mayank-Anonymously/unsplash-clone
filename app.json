      "adaptiveIcon": {
        "foregroundImage": "./assets/Unsplash_Symbol.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mayank221.unsplashclone"
    },
    "web": {
      "favicon": "./assets/Unsplash_Symbol.png"
    },
    "extra": {
      "eas": {
        "projectId": "3592d5b8-94d9-4d4f-ad7b-67583c2c07ed"
      }
    }
  }
