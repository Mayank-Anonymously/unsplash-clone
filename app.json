{
  "expo": {
    "name": "unsplash-clone",
    "slug": "unsplash-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Unsplash_Symbol.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Unsplash_Logo_Full.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
